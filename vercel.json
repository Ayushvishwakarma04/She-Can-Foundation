{
  "builds": [
    {
      "src": "FrontEnd/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "Backend/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "FrontEnd/dist/$1"
    }
  ]
}